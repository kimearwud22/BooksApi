{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "indent": "off",
    "eol-last": "off",
    "quotes": "off",
    "no-else-return": "off",
    "object-curly-newline": "off",
    "no-unused-vars": "off"
  }
}
